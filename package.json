{
  "name": "mini-test-agents",
  "version": "1.0.0",
  "description": "Multi-Agent System Test Project - Phase 1",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:frontend": "jest tests/frontend.test.js",
    "test:integration": "jest tests/integration-full.test.js",
    "serve": "cd public && python3 -m http.server 8080",
    "test-automation": "node .github/scripts/test-automation.js",
    "validate": "node .github/scripts/test-automation.js",
    "setup": "chmod +x scripts/*.sh && ./scripts/setup.sh",
    "deploy": "./scripts/deploy.sh",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health": "curl -f http://localhost:3000/health && curl -f http://localhost:8080/",
    "clean": "rm -rf node_modules coverage logs tmp && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nicolas6910/mini-test-agents.git"
  },
  "keywords": [
    "multi-agent",
    "automation",
    "github",
    "coordination",
    "api",
    "backend",
    "express"
  ],
  "author": "Multi-Agent System",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nicolas6910/mini-test-agents/issues"
  },
  "homepage": "https://github.com/Nicolas6910/mini-test-agents#readme",
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "jsdom": "^23.0.1",
    "node-fetch": "^2.7.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/main.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}